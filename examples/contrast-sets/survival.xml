<?xml version="1.0"?>
<experiment>
<parameter_sets>

<parameter_set name="5 passes, p_strength 0.5, p_sat 0.2, decreasing">		
		<param name="mincov_all_list">0.8 0.5 0.2 0.1</param>
		<param name="mincov_new">0.1</param>
		<param name="maxcov_negative">0.5</param>
		<param name="penalty_strength">0.5</param>
		<param name="penalty_saturation">0.2</param>
		<param name="select_best_candidate">true</param>
		<param name="max_passes_count">5</param>
	</parameter_set>

</parameter_sets>

